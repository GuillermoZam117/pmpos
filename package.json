{
  "name": "react-boilerplate",
  "private": true,
  "version": "2.5.6",
  "description": "Boilerplate for [SurviveJS - React](http://survivejs.com/react/introduction/)",
  "scripts": {
    "stats": "webpack --profile --json > stats.json",
    "start": "webpack serve --mode development --open",
    "deploy": "gh-pages -d build",
    "build": "webpack --mode production",
    "test": "karma start",
    "test:tdd": "npm run test -- --auto-watch --no-single-run",
    "test:lint": "eslint ./app ./tests --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern dist --cache",
    "debug": "cross-env NODE_ENV=development webpack serve --mode development --env debug=true",
    "debug:api": "node --experimental-json-modules --no-warnings scripts/debug-sambapos.mjs"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@hot-loader/react-dom": "^17.0.2",
    "@types/react": "^17.0.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.1",
    "dotenv": "^16.4.7",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.37.4",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "node-fetch": "^2.7.0",
    "react-hot-loader": "^4.13.1",
    "redux": "^5.0.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.97.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.14",
    "@mui/material": "^5.16.14",
    "@mui/styles": "^5.15.18",
    "@redux-devtools/extension": "^3.3.0",
    "classnames": "^2.2.6",
    "core-js": "^3.37.1",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "immutability-helper": "^3.1.1",
    "immutable": "^4.3.7",
    "jquery": "^3.5.0",
    "ms-signalr-client": "^2.2.2",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-addons-pure-render-mixin": "^15.6.3",
    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.3",
    "react-redux": "^7.2.9",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0",
    "regenerator-runtime": "^0.14.1",
    "uuid": "^8.3.2"
  }
}
